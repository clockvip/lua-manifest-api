{
  "functions": {
    "api/[appId].lua.js": {
      "maxDuration": 10,
      "memory": 1024
    },
    "api/health.js": {
      "maxDuration": 5,
      "memory": 128
    }
  },
  "routes": [
    {
      "src": "/(\\d+)\\.lua",
      "dest": "/api/[appId].lua.js",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/",
      "dest": "/api/health.js"
    }
  ]
}